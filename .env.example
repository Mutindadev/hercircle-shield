# Database Configuration
DATABASE_URL=mysql://user:password@localhost:3306/hercircle_shield

# Authentication - JWT
JWT_SECRET=your_secure_jwt_secret_minimum_32_characters_long_please_change_this

# Google OAuth 2.0 Configuration
# Get these from https://console.cloud.google.com
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK_URL=http://localhost:3000/api/auth/google/callback

# Application
NODE_ENV=development
VITE_APP_ID=hercircle-shield

# AI Services
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Manus Forge API (for LLM services)
BUILT_IN_FORGE_API_URL=https://forge.manus.im
BUILT_IN_FORGE_API_KEY=your_forge_api_key

# Optional: Owner Information
OWNER_OPEN_ID=your_owner_google_id

# Server Configuration
PORT=3000
